{
  "short_name": "Pine",
  "name": "Pine Reader",
  "version": "2.0.0",
  "id": "com.thepineapple.pinereader",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["standalone", "fullscreen", "window-controls-overlay"],
  "orientation": "portrait-primary",
  "theme_color": "#000000",
  "background_color": "#FFF8E1",
  "android_config": "android-config.json",
  "icons": [
    {
      "src": "/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    }
  ]
}